            },
            {
              "key": "curSessions",
              "label": "当前会话"
            },
            {
              "key": "backups",
              "label": "备份索引"
            },
            {
              "key": "appSubCaches",
              "label": "应用订阅缓存"
            }
          ]
        },
        {
          "id": "@gist.username",
          "name": "用户名",
          "val": null,
          "type": "text",
          "placeholder": "github 用户名",
          "desc": "必填"
